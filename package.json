{
  "name": "react-learning",
  "version": "2.0.0",
  "description": "Learning React",
  "scripts": {
    "start:dev": "webpack serve",
    "build:dev": "webpack",
    "build:prod": "webpack --env production",
    "lint": "run-p -n -l lint:eslint lint:stylelint",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "lint:fix:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:fix:stylelint": "stylelint \"src/**/*.scss\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\""
  },
  "author": "Borislav Draganov",
  "license": "MIT",
  "repository": "git@github.com:borislav-draganov/react-learning.git",
  "devDependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/compression-webpack-plugin": "^6.0.1",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.0.0",
    "copy-webpack-plugin": "^6.4.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.0.0-beta.0",
    "webpack-merge": "^5.4.1"
  },
  "pre-commit": [
    "format"
  ]
}
