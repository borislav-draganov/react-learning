{
    "name": "react-learning",
    "version": "2.1.0",
    "description": "Learning React",
    "scripts": {
        "start:dev": "webpack serve",
        "build:dev": "webpack",
        "build:prod": "webpack --env production",
        "lint": "run-p -n -l lint:eslint lint:stylelint",
        "lint:eslint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:stylelint": "stylelint \"src/**/*.scss\"",
        "lint:fix:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "lint:fix:stylelint": "stylelint \"src/**/*.scss\" --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\""
    },
    "author": "Borislav Draganov",
    "license": "MIT",
    "repository": "git@github.com:borislav-draganov/react-learning.git",
    "devDependencies": {
        "@types/react": "^18.0.5",
        "@types/react-dom": "^18.0.1",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^9.2.0",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.7.1",
        "eslint": "^8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react": "^7.29.4",
        "html-webpack-plugin": "^5.5.0",
        "npm-run-all": "^4.1.5",
        "pre-commit": "^1.2.2",
        "prettier": "^2.6.2",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "^6.3.0",
        "redux": "^4.1.2",
        "redux-thunk": "^2.4.1",
        "sass": "^1.50.0",
        "sass-loader": "^12.6.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.7.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^25.0.0",
        "terser-webpack-plugin": "^5.3.1",
        "ts-loader": "^9.2.8",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1",
        "webpack-merge": "^5.8.0"
    },
    "pre-commit": [
        "format"
    ],
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.1",
        "@types/react-redux": "^7.1.24",
        "react-redux": "^8.0.0"
    }
}
